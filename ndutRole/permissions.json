[
  {
    "type": "rest",
    "subject": "/mine",
    "action": "read"
  },
  {
    "type": "rest",
    "subject": ["/team", "/team-user"],
    "action": ["read", "create"],
    "conditions": {
      "access": { "$in": ["admin"] }
    }
  },
  {
    "type": "rest",
    "subject": ["/team/:id", "/team-user/:id"],
    "action": ["read", "update", "remove"],
    "conditions": {
      "access": { "$in": ["admin"] }
    }
  }
]